{
  "name": "no",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:all": "npm run build && npm run server:build && npm run build:copy",
    "build:copy": "node -e \"const fs=require('fs');const path=require('path');const copyDir=(src,dest)=>{if(!fs.existsSync(dest))fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src).forEach(file=>{const srcPath=path.join(src,file);const destPath=path.join(dest,file);fs.statSync(srcPath).isDirectory()?copyDir(srcPath,destPath):fs.copyFileSync(srcPath,destPath);});};copyDir('dist','build/frontend');copyDir('dist-server','build/backend');console.log('âœ… Build artifacts copied to build/ directory');\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "nodemon --exec \"ts-node --project tsconfig.server.json server/index.ts\"",
    "server:build": "npx tsc --project tsconfig.server.json",
    "start": "node dist-server/index.js",
    "test:login": "ts-node --project tsconfig.server.json scripts/testLoginFlow.ts",
    "deploy:cpanel": "ts-node --project tsconfig.server.json scripts/deployToCpanel.ts",
    "zip:deploy": "node -e \"try{require('fs').rmSync('itp-notification-deploy.zip',{force:true});}catch(e){}\" && bestzip itp-notification-deploy.zip deploy/*",
    "zip:frontend": "node -e \"try{require('fs').rmSync('itp-frontend-public.zip',{force:true});}catch(e){}\" && bestzip itp-frontend-public.zip deploy/frontend_package/*",
    "zip:build": "npm run build:all && node -e \"try{require('fs').rmSync('build-package.zip',{force:true});}catch(e){}\" && bestzip build-package.zip build/*"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@types/node-fetch": "^2.6.13",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.2",
    "twilio": "^5.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.1",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/ssh2": "^1.15.5",
    "@types/ssh2-sftp-client": "^9.0.5",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "bestzip": "^2.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "mongodb-memory-server": "^10.2.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "ssh2": "^1.17.0",
    "ssh2-sftp-client": "^12.0.1",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
